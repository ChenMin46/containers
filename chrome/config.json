{
    "version": "0.2.0",
    "platform": {
        "os": "linux",
        "arch": "x86_64"
    },
    "process": {
        "terminal": false,
        "user": {
            "uid": 0,
            "gid": 0,
            "additionalGids": [
                29,
                44
            ]
        },
        "args": [
            "google-chrome",
            "--user-data-dir=/data",
            "--proxy-server=",
            "--host-resolver-rules="
        ],
        "env": [
            "DISPLAY=unix:0.0",
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
        ],
        "cwd": ""
    },
    "root": {
        "path": "rootfs",
        "readonly": false
    },
    "hostname": "chrome",
    "mounts": [
        {
            "name": "/root/Pictures",
            "path": "/root/Pictures"
        },
        {
            "name": "/root/Torrents",
            "path": "/root/Torrents"
        },
        {
            "name": "/data",
            "path": "/data"
        },
        {
            "name": "/dev/shm",
            "path": "/dev/shm"
        },
        {
            "name": "/etc/hosts",
            "path": "/etc/hosts"
        },
        {
            "name": "/etc/localtime",
            "path": "/etc/localtime"
        },
        {
            "name": "/tmp/.X11-unix",
            "path": "/tmp/.X11-unix"
        },
        {
            "name": "/root/Downloads",
            "path": "/root/Downloads"
        },
        {
            "name": "/proc",
            "path": "/proc"
        },
        {
            "name": "/dev",
            "path": "/dev"
        },
        {
            "name": "/dev/pts",
            "path": "/dev/pts"
        },
        {
            "name": "/dev/mqueue",
            "path": "/dev/mqueue"
        },
        {
            "name": "/sys",
            "path": "/sys"
        },
        {
            "name": "/sys/fs/cgroup",
            "path": "/sys/fs/cgroup"
        },
        {
            "name": "/etc/resolv.conf",
            "path": "/etc/resolv.conf"
        }
    ],
    "linux": {
        "capabilities": [
            "CAP_CHOWN",
            "CAP_DAC_OVERRIDE",
            "CAP_FSETID",
            "CAP_FOWNER",
            "CAP_MKNOD",
            "CAP_NET_RAW",
            "CAP_SETGID",
            "CAP_SETUID",
            "CAP_SETFCAP",
            "CAP_SETPCAP",
            "CAP_NET_BIND_SERVICE",
            "CAP_SYS_CHROOT",
            "CAP_KILL",
            "CAP_AUDIT_WRITE"
        ]
    }
}

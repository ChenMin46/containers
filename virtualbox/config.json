{
    "version": "0.2.0",
    "platform": {
        "os": "linux",
        "arch": "x86_64"
    },
    "process": {
        "terminal": false,
        "user": {
            "uid": 0,
            "gid": 0
        },
        "args": [
            "/usr/bin/virtualbox"
        ],
        "env": [
            "DISPLAY=unix:0.0",
            "GDK_SCALE=2",
            "GDK_DPI_SCALE=0.5",
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
        ],
        "cwd": "/"
    },
    "root": {
        "path": "rootfs",
        "readonly": false
    },
    "mounts": [
        {
            "name": "/etc/localtime",
            "path": "/etc/localtime"
        },
        {
            "name": "/tmp/.X11-unix",
            "path": "/tmp/.X11-unix"
        },
        {
            "name": "/root/VirtualBox VMs",
            "path": "/root/VirtualBox VMs"
        },
        {
            "name": "/root/.config/VirtualBox",
            "path": "/root/.config/VirtualBox"
        },
        {
            "name": "proc",
            "path": "/proc"
        },
        {
            "name": "dev",
            "path": "/dev"
        },
        {
            "name": "devpts",
            "path": "/dev/pts"
        },
        {
            "name": "shm",
            "path": "/dev/shm"
        },
        {
            "name": "mqueue",
            "path": "/dev/mqueue"
        },
        {
            "name": "sysfs",
            "path": "/sys"
        },
        {
            "name": "cgroup",
            "path": "/sys/fs/cgroup"
        }
    ],
    "linux": {
        "capabilities": [
            "CAP_CHOWN",
            "CAP_DAC_OVERRIDE",
            "CAP_DAC_READ_SEARCH",
            "CAP_FOWNER",
            "CAP_FSETID",
            "CAP_KILL",
            "CAP_SETGID",
            "CAP_SETUID",
            "CAP_SETPCAP",
            "CAP_LINUX_IMMUTABLE",
            "CAP_NET_BIND_SERVICE",
            "CAP_NET_BROADCAST",
            "CAP_NET_ADMIN",
            "CAP_NET_RAW",
            "CAP_IPC_LOCK",
            "CAP_IPC_OWNER",
            "CAP_SYS_MODULE",
            "CAP_SYS_RAWIO",
            "CAP_SYS_CHROOT",
            "CAP_SYS_PTRACE",
            "CAP_SYS_PACCT",
            "CAP_SYS_ADMIN",
            "CAP_SYS_BOOT",
            "CAP_SYS_NICE",
            "CAP_SYS_RESOURCE",
            "CAP_SYS_TIME",
            "CAP_SYS_TTY_CONFIG",
            "CAP_MKNOD",
            "CAP_LEASE",
            "CAP_AUDIT_WRITE",
            "CAP_AUDIT_CONTROL",
            "CAP_SETFCAP",
            "CAP_MAC_OVERRIDE",
            "CAP_MAC_ADMIN",
            "CAP_SYSLOG",
            "CAP_WAKE_ALARM",
            "CAP_BLOCK_SUSPEND",
            "CAP_AUDIT_READ"
        ]
    }
}

{
    "version": "0.2.0",
    "platform": {
        "os": "linux",
        "arch": "x86_64"
    },
    "process": {
        "terminal": true,
        "user": {
            "uid": 0,
            "gid": 0
        },
        "args": [
            "irssi"
        ],
        "env": [
            "DBUS_SESSION_DBUS_ADDRESS=unix:abstract=/home/user/.dbus/session-bus/c229462c5b48459d87bd86a281fdfb10-unix_0",
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "HOME=/home/user",
            "LANG=C.UTF-8",
            "IRSSI_VERSION=0.8.17",
            "LIB_OTR_VERSION=4.1.0",
            "IRSSI_OTR_VERSION=1.0.0",
            "TERM=xterm"
        ],
        "cwd": "/home/user"
    },
    "root": {
        "path": "rootfs",
        "readonly": false
    },
    "hostname": "irssi",
    "mounts": [
        {
            "name": "/etc/localtime",
            "path": "/etc/localtime"
        },
        {
            "name": "/tmp/.X11-unix",
            "path": "/tmp/.X11-unix"
        },
        {
            "name": "/home/user/.cache/dconf",
            "path": "/home/user/.cache/dconf"
        },
        {
            "name": "/home/user/.dbus",
            "path": "/home/user/.dbus"
        },
        {
            "name": "/etc",
            "path": "/etc"
        },
        {
            "name": "/home/user/.irssi",
            "path": "/home/user/.irssi"
        },
        {
            "name": "proc",
            "path": "/proc"
        },
        {
            "name": "dev",
            "path": "/dev"
        },
        {
            "name": "devpts",
            "path": "/dev/pts"
        },
        {
            "name": "shm",
            "path": "/dev/shm"
        },
        {
            "name": "mqueue",
            "path": "/dev/mqueue"
        },
        {
            "name": "sysfs",
            "path": "/sys"
        },
        {
            "name": "cgroup",
            "path": "/sys/fs/cgroup"
        },
        {
            "name": "/etc/hosts",
            "path": "/etc/hosts"
        },
        {
            "name": "/etc/resolv.conf",
            "path": "/etc/resolv.conf"
        }
    ],
    "linux": {
        "capabilities": [
            "CAP_CHOWN",
            "CAP_DAC_OVERRIDE",
            "CAP_FSETID",
            "CAP_FOWNER",
            "CAP_MKNOD",
            "CAP_NET_RAW",
            "CAP_SETGID",
            "CAP_SETUID",
            "CAP_SETFCAP",
            "CAP_SETPCAP",
            "CAP_NET_BIND_SERVICE",
            "CAP_SYS_CHROOT",
            "CAP_KILL",
            "CAP_AUDIT_WRITE"
        ]
    }
}